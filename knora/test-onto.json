              },
              "gui_element": "Spinbox",
              "gui_attributes": ["min=0", "max=9"],
              "cardinality": "1"
            },
            {
              "name": "listprop",
              "object": "ListValue",
              "labels": {
                "en": "List value"
              },
              "gui_element": "Pulldown",
              "gui_attributes": ["hlist=options"],
              "cardinality": "1"
            },
            {
              "name": "dateprop",
              "object": "DateValue",
              "labels": {
                "en": "Date value"
              },
              "gui_element": "Date",
              "cardinality": "1-n"
            },
            {
              "name": "decimalprop",
              "object": "DecimalValue",
              "labels": {
                "en": "Decimal value"
              },
              "gui_element": "SimpleText",
              "gui_attributes": ["size=16", "maxlength=16"],
